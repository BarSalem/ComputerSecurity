{
    "password": {
        "length": 8,
        "characters": {
            "regex": "^[A-Za-z\\d@$!%*?&]+$",
            "uppercase": "[A-Z]",
            "lowercase": "[a-z]",
            "numbers": "\\d",
            "special": "[@$!%*?&]"
        },
        "history_limit": 3,
        "avoid_sequence": [
            "abc",
            "123"
        ]
    },
    "login": {
        "num_of_login_attempts": 3
    }
}